{
  "name": "sf-fx-middleware",
  "version": "1.2.0",
  "description": "Middleware for salesforce functions",
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "build": "node_modules/typescript/bin/tsc -p .",
    "lint": "eslint './*.ts' 'test/**/*.ts' --ext .ts",
    "start": "node dist/index",
    "test": "cross-env USER_FUNCTION_URI=../middleware/test/fixtures/functions/mock nyc --extension .ts mocha --forbid-only -r ts-node/register 'test/unit/**/*.ts'",
    "test-debug": "nyc --extension .ts mocha --inspect-brk=9229 --forbid-only -r ts-node/register 'test/unit/**/*.ts'",
    "watch-node": "nodemon dist/index.js",
    "watch": "tsc -w"
  },
  "pre-commit": [
    "build",
    "lint",
    "test"
  ],
  "repository": {
    "type": "git"
  },
  "author": "TODO",
  "dependencies": {
    "@salesforce/salesforce-sdk": "^1.2.0",
    "@types/node": "^10.17.21",
    "tslib": "1.9.3",
    "typescript": "3.4"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.17.21",
    "@types/sinon": "^7.5.2",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "minimist": ">=1.2.2",
    "mocha": "^5.2.0",
    "nyc": "^15.0.1",
    "pre-commit": "^1.2.2",
    "prettier": "^1.15.3",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.7",
    "rewire": "^5.0.0",
    "sinon": "^7.2.3",
    "ts-node": "^8.9.0"
  }
}
